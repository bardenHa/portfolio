---
import Page from '@/lib/layouts/page.astro';
import Home from '@/views/home';

import { getCollection } from 'astro:content';
import { defaultTo } from 'rambda';
const projects = (await getCollection('projects')).sort((a, b) =>
  defaultTo(new Date(), a.data.startDate) > defaultTo(new Date(), b.data.startDate) ? -1 : 1
);
---

<Page
  title="Harry Barden - Software Engineer"
  description="Harry Barden's personal website."
  keywords="Harry Barden, Software Engineer, Developer, Web Developer, Frontend Developer, Fullstack Developer, React, JavaScript, TypeScript, Node, Express, MongoDB, GraphQL, Apollo, Next.js, Astro, Svelte, Sapper, Vue, Nuxt, Tailwind, CSS, HTML, Webpack, Babel, Git, GitHub, GitLab, Bitbucket, Docker, Kubernetes, AWS, GCP, Google Cloud, Cloudflare, Netlify, Vercel, Heroku, Digital Ocean, Linode, Ubuntu, Linux, macOS, Windows, VS Code, Visual Studio Code, Vim, Terminal, Bash, Zsh, Fish, Fish Shell, Oh My Zsh, Oh My Fish, iTerm"
  image="/images/og.png"
  type="website"
>
  <Home projects={projects} />
</Page>
