{
  "name": "bardenha-portfolio",
  "version": "0.0.0",
  "description": "My personal portfolio and blog",
  "repository": {
    "url": "https://github.com/bardenha/portfolio"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check && pnpm run typecheck && pnpm run lint && pnpm run format && pnpm run build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,astro,json,css,scss,md}\" --plugin=prettier-plugin-astro --plugin=prettier-plugin-tailwindcss",
    "typecheck": "tsc --noEmit",
    "analyse:bundle": "VITE_OPEN_VISUALIZER=true vite build"
  },
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/prefetch": "^0.3.0",
    "@astrojs/sitemap": "^2.0.2",
    "@astrojs/solid-js": "^2.2.1",
    "@astrojs/tailwind": "^4.0.0",
    "@stitches/core": "^1.2.8",
    "@thoughtbot/tailwindcss-aria-attributes": "^0.2.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.4.7",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "astro": "^2.10.12",
    "class-variance-authority": "^0.7.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-solid": "^0.12.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.0.2",
    "prettier-plugin-astro": "^0.11.1",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "rambda": "^8.3.0",
    "rehype-prism-plus": "^1.6.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "solid-icons": "^1.0.8",
    "solid-js": "^1.7.6",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  }
}
